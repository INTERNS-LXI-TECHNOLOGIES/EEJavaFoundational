	</servlet-mapping>

	 <servlet>
		<servlet-name>Result</servlet-name>
		<servlet-class>com.lxisoft.servlet.ResultServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Result</servlet-name>
		<url-pattern>/viewMark</url-pattern>
	</servlet-mapping>


	 <servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.lxisoft.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/logining</url-pattern>
	</servlet-mapping>


	 <servlet>
		<servlet-name>Delete</servlet-name>
		<servlet-class>com.lxisoft.servlet.DeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Delete</servlet-name>
		<url-pattern>/problemDelete</url-pattern>
	</servlet-mapping>

 <servlet>
		<servlet-name>Edit</servlet-name>
		<servlet-class>com.lxisoft.servlet.EditServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Edit</servlet-name>
		<url-pattern>/questionEdit</url-pattern>
	</servlet-mapping>

</web-app>
