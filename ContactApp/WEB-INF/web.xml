<web-app>
<servlet>
	<servlet-name>Signin</servlet-name>
	<servlet-class>com.lxisoft.servlets.SignupServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Signin</servlet-name>
	<url-pattern>/signup</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>AddContact</servlet-name>
	<servlet-class>com.lxisoft.servlets.AddContactServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>AddContact</servlet-name>
	<url-pattern>/add</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>Login</servlet-name>
	<servlet-class>com.lxisoft.servlets.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Login</servlet-name>
	<url-pattern>/login</url-pattern>
</servlet-mapping>
</web-app>